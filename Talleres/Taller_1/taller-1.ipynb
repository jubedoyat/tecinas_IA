    "def compareHistograms(a, b):\n",
    "    comparisons = []\n",
    "    for method in range(4):\n",
    "        d = cv.compareHist(a, b, method)\n",
    "        comparisons.append(d)\n",
    "    return comparisons"
   ]
  },
  {
   "cell_type": "code",
